{
	"name": "weboftalent/staff",
	"description": "Simple module to store and render members of staff for an organisation",
	"type": "silverstripe-module",
	"keywords": [
		"silverstripe",
		"staff",
		"people",
		"weboftalent"
	],
	"authors": [{
		"name": "Gordon Anderson",
		"email": "gordon.b.anderson@gmail.com",
		"homepage": "https://github.com/gordonbanderson/gordonbanderson/weboftalent-staff",
		"role": "Developer"
	}],
	"support": {
		"issues": "https://github.com/gordonbanderson/weboftalent-staff/issues"
	},
	"require": {
		"php" : "~5.6|~7.0",
		"silverstripe/framework": "^4.1",
		"weboftalent/portlets": "^4.0"
	},
	"require-dev": {
		"phpunit/phpunit": "^5.7",
		"squizlabs/php_codesniffer": "^2.3"
	},
	"autoload": {
		"psr-4": {
			"WebOfTalent\\Staff\\": "src/",
			"WebOfTalent\\Staff\\Tests\\": "tests/"
		}
	},
	"extra": {
		"expose": [
			"icons"
		]
	},
	"license": "BSD-3-Clause",
	"minimum-stability": "dev",
	"prefer-stable": true
}
